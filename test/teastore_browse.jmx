          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="Create Product Images Request Body" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">// Import necessary classes
import groovy.json.JsonBuilder

// Initialize the JSON object
def jsonObject = new JsonBuilder()
def idsMap = [:] // Create an empty map to hold the ID-value pairs

// Loop through the IDs from ids_1 to ids_20
for (int i = 1; i &lt;= 20; i++) {
    def id = vars.get(&quot;product_ids_&quot; + i) // Get the current ID
    if (id != null) {
        idsMap[id] = &quot;64x64&quot; // Add to the map with the required value
    }
}

// Create the JSON object from the map
jsonObject(idsMap)

// Store the result in a variable
vars.put(&quot;product_images_request_body&quot;, jsonObject.toPrettyString()) // Save the JSON string</stringProp>
          </JSR223PreProcessor>
